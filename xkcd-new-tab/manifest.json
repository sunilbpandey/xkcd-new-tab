{
    "manifest_version": 3,
    "name": "xkcd New Tab",
    "description": "See a random xkcd comic when you open a new tab.",
    "version": "1.0.1",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "host_permissions": [
        "https://xkcd.com/*"
    ],
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}